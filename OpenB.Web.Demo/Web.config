<?xml version="1.0"?>
<configuration>
  <system.webServer>   
    <modules>
      <add name="OpenBModule" type="OpenB.Web.Http.WebRequestModule, OpenB.Web"/>
    </modules>
    <handlers>
      <remove name="StaticFile"/>
      <add name="OpenBHandler" verb="*" path="*" type="OpenB.Web.Http.HttpHandlerFactory, OpenB.Web" resourceType="Unspecified"/>
    </handlers>   
  </system.webServer>
  <system.web>
    <compilation debug="true"/>
  </system.web>
</configuration>

